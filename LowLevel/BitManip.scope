func str toBin(int num) {
	str out = "";
	for (int i : 0..65) {
		int v = nthBit(i, num);

		// Insert 0 or 1
		if (v == 0) {
			out = "0" + out;
		} else {
			out = "1" + out;
		}
	}
	ret out;
}

func dec rawCastIntToDec(int a) {
	dec o = 0.0;
	assembly {
		vlist_getptr rdi, $a$
		mov rsi, 0
		vlist_set $o$
	}
	ret o;
}

func int rawCastDecToInt(dec a) {
	int o = 0;
	assembly {
		vlist_getptr rdi, $a$
		mov rsi, 0
		vlist_set $o$
	}
	ret o;
}

func int nthBit(int n, int a) {
	ret and(shr(a, n), 1);
}

func int and(int a, int b) {
	int o = 0;
	assembly {
		vlist_getptr rdi, $a$
		vlist_getptr rdx, $b$
		mov rsi, 0
		and rdi, rdx
		vlist_set $o$
	}
	ret o;
}


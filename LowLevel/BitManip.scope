func int shl(int a, int b) {
	int o = 0;
	assembly {
		vlist_getptr rdi, $a$
		vlist_getptr rcx, $b$
		mov rsi, 0
		shl rdi, cl
		vlist_set $o$
	}
	ret o;
}

